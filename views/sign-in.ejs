<% include header %>
	<div class="form-group">
		<form name="signInForm" action="sign-in" method="post" class="form-group">
			<label for="Username">Username</label>
			<input name="username" placeholder="enter username" class="form-control"><br>
			<label for="Password">Password</label>
			<input name="password" placeholder="enter password" class="form-control"><br>
			<input type="submit" id="Sign In" class="btn btn-primary" value="Sign In">
		</form>
	</div>
	<div class="form-group">
		<form name="signUpForm" action="sign-up" method="post" class="form-group">
			<label for="Username">Username</label>
			<input name="username" placeholder="create username" class="form-control"><br>
			<label for="Password">Password</label>
			<input name="password" placeholder="create password" class="form-control"><br>
			<input type="submit" id="Sign Up" class="btn btn-primary" value="Sign Up">
		</form>
	</div>
<% include footer %>